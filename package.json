{
  "name": "@glitchybyte/dlog",
  "version": "2.1.0",
  "author": "GlitchyByte",
  "license": "Apache-2.0",
  "description": "Console logging for enhancing logs with source filenames and line numbers while removing all logs in production.",
  "keywords": [
    "babel-plugin",
    "vite-plugin",
    "console",
    "dev",
    "log",
    "source",
    "line"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GlitchyByte/dlog.git"
  },
  "files": [
    "dist",
    "src/plugins"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./babel": {
      "default": "./src/plugins/babel-plugin.js",
      "types": "./src/plugins/babel-plugin.d.ts"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsc"
  },
  "peerDependencies": {
    "@babel/core": "^7.26.10",
    "vite": "^6.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "fs-extra": "^11.3.0",
    "typescript": "^5.8.3",
    "vite": "^6.2.2"
  }
}
